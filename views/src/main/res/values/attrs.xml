<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AvatarView">
        <attr name="shape" format="enum">
            <enum name="circle" value="0" />
            <enum name="sexangle" value="1" />
        </attr>
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_radius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MaxHeightNestedScrollView">
        <attr name="maxHeight" format="dimension" />
        <attr name="maxHeightRatio" format="float" />
    </declare-styleable>

    <declare-styleable name="AvatarCropView">
        <attr name="shape_crop" format="enum">
            <enum name="circle" value="0" />
            <enum name="sexangle" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ColumnDiagramView">
        <attr name="text_size" format="dimension" />
        <attr name="foot_height" format="dimension" />
        <attr name="diagram_width" format="dimension" />
    </declare-styleable>

</resources>